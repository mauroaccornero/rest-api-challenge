{
  "name": "rest-api-challenge",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^1.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --open --config webpack.dev.js",
    "build": "rimraf ./build && cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "build:dev": "npm run build -- --env API_URL=http://localhost:4000",
    "serve:build": "cross-env NODE_ENV=production serve -s build -l 3000",
    "test": "jest",
    "dev": "npm run mock-db & npm start",
    "audit:prod": "npm audit --production",
    "check-types": "tsc --noEmit",
    "mock-db": "json-server --watch ./mocks/db.js --port 4000",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "e2e:dev": "npm run mock-db & npm run build:dev & npm run serve:build & cypress open --config-file cypress.config.ts",
    "precommit": "npm run lint:fix && npm run check-types && npm run audit:prod && npm run test",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@faker-js/faker": "^7.6.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "babel-jest": "^29.2.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cypress": "^10.11.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-stylelint": "^17.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^29.2.2",
    "json-server": "^0.17.1",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "serve": "^14.0.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
